{
  "Version": "2012-10-17",
  "Id": "$(businessUserID)-$(genUUID)",
  "Statement": [
    {
      "Sid": "$(businessUserName)-RBAP-admin",
      "Effect": "Allow",
      "Action": "*",
      "Principal": {
        "AWS": "arn:aws:iam::$(MinioAdminId):root"
      },
      "Resource": [
        "arn:aws:s3:::$(bucketName)/*",
        "arn:aws:s3:::$(bucketName)"
      ],
    },
    {
      "Sid": "$(businessUserName)-RBAP-AccessKeyIDSecret-WithOneBucketObject*",
      "Effect": "Allow",
      "Action": "*",
      "Principal": {
        "AWS": "arn:aws:iam::$(MinioUserId):root"
      },
      "Resource": [
        "arn:aws:s3:::$(bucketName)/*"
      ],
    },
  ]
}
