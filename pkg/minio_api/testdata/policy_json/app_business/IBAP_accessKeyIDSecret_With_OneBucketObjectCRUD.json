{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "$(businessUserName)-IBAP-AccessKeyIDSecret-WithOneBucketObject*",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:PutObject",
      ],
      "Resource": [
        "arn:aws:s3:::$(bucketName)/*"
      ]
    }
  ]
}